{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Agent",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/agents/agents_as_tools/agent.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Python: DevUI",
            "type": "debugpy",
            "request": "launch",
            "program": "/home/vscode/.local/bin/devui",
            "args": [
                "./src/agents/",
                "--port=8093",
                "--tracing"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        }
    ]
}
